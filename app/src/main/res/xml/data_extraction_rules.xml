<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="database" path="efb.db" />
        <exclude domain="database" path="efb.db-shm" />
        <exclude domain="database" path="efb.db-wal" />
    </cloud-backup>
    <device-transfer>
        <include domain="sharedpref" path="." />
        <include domain="file" path="." />
    </device-transfer>
</data-extraction-rules>
